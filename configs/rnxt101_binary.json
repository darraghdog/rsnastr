{
    "network": "RSNAClassifier",
    "encoder": "resnext101_32x8d",
    "batches_per_epoch": 600,
    "size": 320,
    "classes": ["pe_present_on_image"],
    "fp16": true,
    "studynmin": 4,
    "studynmax": 8,
    "pe_ratio": 0.5,
    "optimizer": {
        "type": "Adam",
