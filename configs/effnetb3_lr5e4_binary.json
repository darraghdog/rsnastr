{
    "network": "RSNAClassifier",
    "encoder": "tf_efficientnet_b3_ns",
    "batches_per_epoch": 500,
    "size": 320,
    "classes": ["pe_present_on_image"],
    "fp16": true,
    "studynmin": 2,
    "studynmax": 8,
    "pe_ratio": 0.25,
    "optimizer": {
        "type": "Adam",
        "learning_rate": 0.0005,
        "schedule": {
            "type": "steplr",
            "mode": "step",
            "epochs": 30,
            "params": {"step_size": 500, "gamma": 0.9}
        }
    },
    "normalize": {
        "mean": [0.22363983, 0.18190407, 0.2523437 ],
        "std": [0.32451536, 0.2956294,  0.31335256]
    },
    "losses": {
        "FocalBCEWithLogitsLoss": [1.0]
    }
}
